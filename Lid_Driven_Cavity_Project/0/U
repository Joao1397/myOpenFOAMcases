FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    Moving_Wall
    {
        type            fixedValue;
        value           uniform (1 0 0);
    }

    Fixed_Walls
    {
        type            noSlip;
    }

    Front_And_Back
    {
        type            empty;
    }
}

// ************************************************************************* //
